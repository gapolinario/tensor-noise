Cell[BoxData[
 RowBox[{
  RowBox[{"LDLT", "[", 
   RowBox[{"mat_", "?", "SymmetricMatrixQ"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"n", "=", 
       RowBox[{"Length", "[", "mat", "]"}]}], ",", 
      RowBox[{"mt", "=", "mat"}], ",", "v", ",", "w"}], "}"}], ",", 
    RowBox[{
     RowBox[{"Do", "[", 
