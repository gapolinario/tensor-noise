               RowBox[{";;", 
                RowBox[{"j", "-", "1"}]}]}], "]"}], "]"}]}], ";", 
           RowBox[{"v", "=", 
            RowBox[{"w", " ", 
             RowBox[{"Take", "[", 
              RowBox[{
               RowBox[{"Diagonal", "[", "mt", "]"}], ",", 
               RowBox[{"j", "-", "1"}]}], "]"}]}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"mt", "[", 
             RowBox[{"[", 
              RowBox[{"j", ",", "j"}], "]"}], "]"}], "-=", 
            RowBox[{"w", ".", "v"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"j", "<", "n"}], ",", 
             RowBox[{
              RowBox[{"mt", "[", 
               RowBox[{"[", 
                RowBox[{
                 RowBox[{
                  RowBox[{"j", "+", "1"}], ";;"}], ",", "j"}], "]"}], "]"}], "-=", 
              RowBox[{
               RowBox[{"mt", "[", 
                RowBox[{"[", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"j", "+", "1"}], ";;"}], ",", " ", 
                  RowBox[{";;", 
                   RowBox[{"j", "-", "1"}]}]}], "]"}], "]"}], ".", "v"}]}]}], 
            "]"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"mt", "[", 
          RowBox[{"[", 
           RowBox[{
            RowBox[{
             RowBox[{"j", "+", "1"}], ";;"}], ",", "j"}], "]"}], "]"}], "/=", 
         
         RowBox[{"mt", "[", 
          RowBox[{"[", 
           RowBox[{"j", ",", "j"}], "]"}], "]"}]}]}], ",", 
       RowBox[{"{", 
        RowBox[{"j", ",", "n"}], "}"}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"LowerTriangularize", "[", 
